<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Exress Skills</title>
</head>
<body>
    <h1>Express Skills</h1>
    <ul>
        <% skills.forEach(function(s) { %>
            <li>
                <a href="/skills/<%= s.id %>">
                <%= s.id %>
                </a>
                <%= s.description %>
                    -
                <%= s.hard ? 'hard' : 'easy' %>
            </li>
            <% }); %>
    </ul>
</body>
</html>